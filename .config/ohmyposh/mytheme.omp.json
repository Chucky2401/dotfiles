{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "❱❱ ",
    "foreground": "green",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "❱ ",
    "foreground": "green",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "template": "\ue70f"
          },
          "foreground": "cyan",
          "type": "os",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "trailing_diamond": ":",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}TBW@{{ .HostName }} \ue0b1",
          "foreground": "#16C60C",
          "background": "transparent",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uf115 ",
            "folder_separator_icon": "/",
            "home_icon": "󰋜 ",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} $ ",
          "foreground": "#3B78FF",
          "background": "transparent",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} | {{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf0c7 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#95ffa4",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "filler": "<#ffffff,transparent>·</>",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "status_template": "{{ if eq .Code 0 }} \uf00c {{ else }} \uf00d {{ .Code }} {{ end }}"
          },
          "template": "{{ .String }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#585858",
          "type": "status",
          "style": "powerline",
          "foreground_templates": [
            "{{ if .Error }}#d30e0e{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#83769c",
          "type": "executiontime",
          "style": "powerline",
          "min_width": 96
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "powershell": "\ue683",
              "pwsh": "\uebc7",
              "zsh": "\uf120"
            }
          },
          "template": " {{ .Name }} ({{ .Version }})",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#0077c2",
          "type": "shell",
          "style": "powerline",
          "min_width": 90
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b4",
          "background": "#2e9599",
          "type": "time",
          "style": "powerline",
          "min_width": 75
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "❱ ",
          "foreground": "green",
          "type": "text",
          "style": "powerline"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "enable_cursor_positioning": true,
  "final_space": true
}
