{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "enable_cursor_positioning": true,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "foreground": "cyan",
          "properties": {
            "template": "\ue70f"
          },
          "style": "powerline"
        },
        {
          "type": "session",
          "background": "transparent",
          "foreground": "#16C60C",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}TBW@{{ .HostName }} \ue0b1",
          "trailing_diamond": ":"
        },
        {
          "type": "path",
          "background": "transparent",
          "foreground": "#3B78FF",
          "properties": {
            "folder_icon": "\uf115 ",
            "folder_separator_icon": "/",
            "home_icon": "\uf7db ",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": " {{ .Path }} $ "
        },
        {
          "type": "git",
          "background": "#95ffa4",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} | {{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}}  {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}}  {{ .WorktreeCount }}{{ end }} ",
          "source": "cli",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "filler": "<#ffffff,transparent>·</>",
      "newline": false,
      "segments": [
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "properties": {
            "status_template": "{{ if eq .Code 0 }}  {{ else }}  {{ .Code }} {{ end }}",
            "always_enabled": true
          },
          "template": "{{ .String }}",
          "foreground": "#ffffff",
          "background": "#585858",
          "foreground_templates": [
            "{{ if .Error }}#d30e0e{{ end }}"
          ]
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ .FormattedMs }} ",
          "powerline_symbol": "\uE0B0",
          "type": "executiontime",
          "min_width": 96
        },
        {
          "background": "#0077c2",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " {{ .Name }} ({{ .Version }})",
          "type": "shell",
          "min_width": 90,
          "properties": {
            "mapped_shell_names": {
              "pwsh": "\uebc7",
              "powershell": "\ue683",
              "zsh": "\uf120"
            }
          }
        },
        {
          "background": "#2e9599",
          "foreground": "#111111",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time",
          "min_width": 75
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "powerline",
          "foreground": "green",
          "template": "❱ ",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "green",
    "template": "❱ "
  },
  "secondary_prompt": {
      "background": "transparent",
      "foreground": "green",
      "template": "❱❱ "
  },
  "final_space": true,
  "version": 2
}

