{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "‚ù±‚ù± ",
    "foreground": "green",
    "background": "transparent"
  },
  "transient_prompt": {
    "template": "‚ù± ",
    "foreground": "green",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "debian": "ÔåÜ ",
            "raspbian": "Ôåï ",
            "windows": "Óúè ",
            "alpine": "ÔåÄ "
          },
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "background": "#54827a",
          "type": "os"
        },
        {
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "trailing_diamond": ":",
          "template": " {{ if .SSHSession }} Û∞åò {{ end }}{{ .UserName }}@{{ first (splitList \".\" .HostName) }} ",
          "background": "#0a5f05",
          "background_templates": [
            "{{ if contains \"root\" .UserName }}#920B16{{ end }}"
          ],
          "type": "session"
        },
        {
          "options": {
            "folder_icon": "ÔÅº ",
            "folder_separator_icon": "/",
            "home_icon": "ÔÄï ",
            "max_depth": 3,
            "style": "agnoster_short",
            "mapped_locations": {
              "/home/docker": "Óû∞ Local",
              "/home/docker_pi3": "Óû∞ Pi3",
              "/home/docker_hp": "Óû∞ HP"
            }
          },
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "template": " {{ .Path }} $ ",
          "background": "#2953b2",
          "type": "path"
        },
        {
          "type": "python",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#100e23",
          "background": "#906cff",
          "template": " Óàµ {{ .Full }} "
        },
        {
          "options": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "fix/*": "üêõ "
            },
            "upstream_icons": {
              "forgejo.sterimed.local": "Ôåµ",
              "lxc-forgejo.intranet.blackwizard.fr": "Ôåµ",
              "forgejo.intranet.blackwizard.fr": "Ôåµ"
            }
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} {{ .BranchStatus }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} | {{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} ÔÉá {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} ÔÜª {{ .WorktreeCount }}{{ end }} ",
          "foreground": "#193549",
          "powerline_symbol": "ÓÇ∞",
          "background": "#68b272",
          "type": "git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#Fccbc2f{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#Fcca366{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B7c5eae{ end }}"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "filler": "<#ffffff,transparent>¬∑</>",
      "segments": [
        {
          "options": {
            "always_enabled": true,
            "status_template": "{{ if eq .Code 0 }} \uf00c {{ else }} \uf00d {{ .Code }} {{ end }}"
          },
          "template": "{{ .String }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#585858",
          "type": "status",
          "style": "powerline",
          "foreground_templates": [
            "{{ if .Error }}#d30e0e{{ end }}"
          ]
        },
        {
          "options": {
            "always_enabled": true
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#83769c",
          "type": "executiontime",
          "style": "powerline",
          "min_width": 96
        },
        {
          "options": {
            "mapped_shell_names": {
              "powershell": "\ue683",
              "pwsh": "\uebc7",
              "zsh": "\uf120"
            }
          },
          "template": " {{ .Name }} ({{ .Version }})",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#0077c2",
          "type": "shell",
          "style": "powerline",
          "min_width": 90
        },
        {
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b4",
          "background": "#2e9599",
          "type": "time",
          "style": "powerline",
          "min_width": 75
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ù± ",
          "foreground": "green",
          "type": "text",
          "style": "powerline"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "enable_cursor_positioning": true,
  "final_space": true
}
